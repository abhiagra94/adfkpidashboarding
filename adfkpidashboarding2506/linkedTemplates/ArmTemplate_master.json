{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfkpidashboarding2506"
		},
		"LS_sgkpidashboarding_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_sgkpidashboarding'"
		},
		"LS_sqldbkpidashboarding_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sqldbkpidashboarding'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source'"
		},
		"LS_sgkpidashboarding_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sgkpidashboarding.dfs.core.windows.net/"
		},
		"LS_sqldbkpidashboarding_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverkpidashboarding.database.windows.net"
		},
		"LS_sqldbkpidashboarding_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldbkpidashboarding"
		},
		"LS_sqldbkpidashboarding_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloud"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mycomputer"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().database}"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloud"
		},
		"source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverkpidashboarding.database.windows.net"
		},
		"source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "source"
		},
		"source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Cloud"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfkpidashboarding2506_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_sgkpidashboarding_accountKey": {
						"value": "[parameters('LS_sgkpidashboarding_accountKey')]"
					},
					"LS_sqldbkpidashboarding_password": {
						"value": "[parameters('LS_sqldbkpidashboarding_password')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"source_password": {
						"value": "[parameters('source_password')]"
					},
					"LS_sgkpidashboarding_properties_typeProperties_url": {
						"value": "[parameters('LS_sgkpidashboarding_properties_typeProperties_url')]"
					},
					"LS_sqldbkpidashboarding_properties_typeProperties_server": {
						"value": "[parameters('LS_sqldbkpidashboarding_properties_typeProperties_server')]"
					},
					"LS_sqldbkpidashboarding_properties_typeProperties_database": {
						"value": "[parameters('LS_sqldbkpidashboarding_properties_typeProperties_database')]"
					},
					"LS_sqldbkpidashboarding_properties_typeProperties_userName": {
						"value": "[parameters('LS_sqldbkpidashboarding_properties_typeProperties_userName')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"source_properties_typeProperties_server": {
						"value": "[parameters('source_properties_typeProperties_server')]"
					},
					"source_properties_typeProperties_database": {
						"value": "[parameters('source_properties_typeProperties_database')]"
					},
					"source_properties_typeProperties_userName": {
						"value": "[parameters('source_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}